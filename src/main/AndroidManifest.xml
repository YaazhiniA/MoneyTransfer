<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.banktransfer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BankTransfer">
        <activity android:name=".upi"></activity>
        <activity android:name=".trans_history" />
        <activity android:name=".balance" />
        <activity android:name=".amount" />
        <activity android:name=".transfer" />
        <activity android:name=".paybill" />
        <activity android:name=".recharge" />
        <activity android:name=".first" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>